#include <stdio.h>

int main()
{
int a,b;
a = 3;
b = 9;

printf("a + b = %d\n" , a + b);
printf("a - b = %d\n" , a - b);
printf("a / b = %d\n" , a / b);
printf("a * b = %d\n" , a * b);

return 0;

}



if we add float then what code looks like


#include <stdio.h>

int main()
{
int a;
float b;
a = 2;
b = 9;

printf("a + b = %f\n" , a + b);
printf("a - b = %f\n" , a - b);
printf("a / b = %f\n" , a / b);
printf("a * b = %f\n" , a * b);

return 0;

}


#include <stdio.h>

int main()
{

int a;
a = 6;
printf("a * 1 = %d\n" , a * 1);
printf("a * 2 = %d\n" , a * 2);
printf("a * 3 = %d\n" , a * 3);
printf("a * 4 = %d\n" , a * 4);
printf("a * 5 = %d\n" , a * 5);
printf("a * 6 = %d\n" , a * 6);
printf("a * 7 = %d\n" , a * 7);
printf("a * 8 = %d\n" , a * 8);
printf("a * 9 = %d\n" , a * 9);
printf("a * 10 = %d\n" , a * 10);
printf("a * 11 = %d\n" , a * 11);
return 0;
}
