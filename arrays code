#include<stdio.h>
int main(){
    int marks[] = {98, 99, 22, 23, 45, 46};
    
    for(int i = 0; i < 6; i++){  // âœ… use 6, not 35
        if(marks[i] < 35){
            printf("%d ", marks[i]);
        }
    }
    
    return 0;
}

//max value finding from arrays

#include <stdio.h>
#include <limits.h>
int main(){
int arr[7] = { 10,4,45,32,55,2,34};
int max = INT_MIN; // sabse chota number ke use kra 
for(int i=0;i<6;i++){
    if(max<arr[i]){
        max = arr[i];
    }
}

printf("%d",max);
return 0;

}

// min value finding from arrays

#include <stdio.h>
#include <limits.h>
int main(){
int arr[7] = { 10,4,45,32,55,2,34};
int min = INT_MAX;
for(int i=0;i<7;i++){
    if(arr[i]<min){
        min = arr[i];
    }
}

printf("%d",min);
return 0;

}
